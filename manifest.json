{
  "name": "Hello Extensions?",
  "description": "Base Level Extension",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup" : "popup.html"
  },

  "background" : {
    "scripts" : "popup.js"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "windows": "Ctrl+Shift+Y",
        "mac": "Command+Shift+Y",
        "chromeos": "Ctrl+Shift+U",
        "linux": "Ctrl+Shift+J"
      },
      "chrome-url-overrides" : {
        "newtab" : "newPage.html"
      }
    }
  }
}
